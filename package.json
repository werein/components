{
  "name": "@werein/components",
  "version": "0.0.0-development",
  "publishConfig": {
    "@werein:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/werein/components.git"
  },
  "peerDependencies": {
    "baseui": "^10.8.0",
    "formik": "^2.2.9",
    "ramda": "^0.28.0",
